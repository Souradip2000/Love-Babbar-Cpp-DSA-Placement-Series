// Creating 2D array
// Initialising the 2D array
// Taking input column-wise and row-wise

// Row-wise sum of a 2-D array:

#include <iostream>
#include <vector>

using namespace std;

int main()
{
    int arr[3][3]={1,2,3,4,5,6,7,8,9};
    vector<int> ans;
    
    for(int i=0;i<3;i++)
    {
        int sum=0;
        for(int j=0;j<3;j++)
        {
            sum+=arr[i][j];
        }
        ans.push_back(sum);
    }
    
    for(int i: ans)
        cout<<i<<" ";
    return 0;
}

