#include <iostream>
#include<math.h>
#include<vector>

using namespace std;

bool binarySearch( vector<int> arr,  int key )
{
  int low = 0;
  int high = (arr.size() - 1);

  while( low <= high )
    {
      int mid = ( low + ((high - low) / 2));
      if( arr[mid] == key )
      {
        return true;
      }
      else if ( arr[mid] < key )
      {
        low = mid + 1;
      }
      else if( arr[mid] > key )
      {
        high = mid - 1;
      }
      mid = ( low + ((high - low)/2 ));
    }
  return false;
}

int main() {
  vector<int> arr = {1,3,4,6,7,9};
  int key = 9;
  if( binarySearch( arr, key ) )
  {
    cout << "Found!";
  }
  else 
  {
    cout << "Not found!";
  }

  return 0;
}
